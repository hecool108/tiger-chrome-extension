{
  "name": "tiger-chrome-extension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --watch",
    "preview": "vite preview",
    "codew": "graphql-codegen --config codegen.ts --watch",
    "start:chrome": "web-ext run --source-dir=dist --target=chromium --start-url=chrome://extensions/ --reload"
  },
  "dependencies": {
    "@apollo/client": "^3.11.4",
    "@chakra-ui/react": "^2.8.2",
    "@codemirror/lang-json": "^6.0.1",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "ahooks": "^3.8.0",
    "framer-motion": "^11.3.24",
    "graphql": "^16.9.0",
    "jsonrepair": "^3.8.0",
    "wouter": "^3.3.1",
    "zustand": "^4.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@parcel/watcher": "^2.4.1",
    "@types/chrome": "^0.0.270",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "typescript": "^4.6.4",
    "vite": "^5.4.0",
    "web-ext": "^8.2.0"
  }
